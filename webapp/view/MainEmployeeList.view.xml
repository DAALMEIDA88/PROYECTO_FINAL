<mvc:View
    controllerName="logaligroup.logalifinal2.controller.MainEmployeeList"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.f">
                <Page id="MainEmployeeList" title="{i18n>employeeList}" showHeader="false" showNavButton="true" navButtonPress=".onNavBack">
                    <content>
                        <f:FlexibleColumnLayout id="fcl" backgroundDesign="Solid" layout="TwoColumnsMidExpanded">
                            <f:beginColumnPages>
                                <mvc:XMLView id="EmployeeList" viewName="logaligroup.logalifinal2.view.EmployeeList"></mvc:XMLView>
                            </f:beginColumnPages>
                            <f:midColumnPages>                           
                                <mvc:XMLView id="detailEmployeeView" viewName="logaligroup.logalifinal2.view.EmployeeDetails"></mvc:XMLView>
                         
                            </f:midColumnPages>
                            <f:endColumnPages>
                                <Page >
                                </Page>
                            </f:endColumnPages>
                        </f:FlexibleColumnLayout>
                    </content>
                </Page>
</mvc:View> 